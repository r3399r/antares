{
  "name": "porrima",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env BROWSER=none PORT=3002 react-scripts start",
    "build": "cross-env BUILD_PATH='./dist' react-scripts build",
    "test": "react-scripts test --coverage",
    "test:nowatch": "npm run test -- --watchAll=false",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --list-different \"{src,public}/**/*.{tsx,ts,js,html,scss,json}\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-s lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist -f",
    "pre:commit": "npm run lint",
    "pre:deploy": "run-s clean build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "classnames": "^2.3.1",
    "mathjs": "^10.4.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "cross-env": "^7.0.3",
    "del-cli": "^4.0.1",
    "eslint": "^8.12.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.4",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest-watch-typeahead": "^0.6.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "sass": "^1.49.10"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "https://r3399r.github.io/porrima/"
}
