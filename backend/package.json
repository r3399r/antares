{
  "name": "antares-service",
  "version": "0.0.1",
  "author": "Yue Chang",
  "scripts": {
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\" --ignore-path .gitignore",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "pre:commit": "run-s lint test:coverage build",
    "pre:dev": "run-s fix test:coverage",
    "compile": "ttsc"
  },
  "license": "ISC",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.31",
    "@types/jest": "^29.2.4",
    "@types/multer": "^1.4.7",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.11.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^29.3.1",
    "multer": "^1.4.5-lts.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.7.4",
    "typescript-transform-paths": "^3.3.1"
  }
}
